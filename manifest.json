{
    "manifest_version": 3,
    "name": "ScamCheck",
    "version": "1.0",
    "description": "Checks if a website is likely to be a scam using WHOIS data.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://*.whoisxmlapi.com/*"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "scam.png"
    }
  }
  